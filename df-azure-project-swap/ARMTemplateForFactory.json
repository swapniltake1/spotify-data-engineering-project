									"inputs": [
										{
											"referenceName": "Json_dynamic",
											"type": "DatasetReference",
											"parameters": {
												"container": "bronze",
												"folder": {
													"value": "@{pipeline().parameters.table}_CDC",
													"type": "Expression"
												},
												"cdc": "empty.json"
											}
										}

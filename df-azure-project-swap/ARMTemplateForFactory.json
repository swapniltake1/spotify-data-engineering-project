						"userProperties": [],
						"typeProperties": {
							"variableName": "current",
							"value": {
								"value": "@utcNow()",
								"type": "Expression"
							}
						}
					},
					{
						"name": "ifincreamentaldata",
						"description": "",
						"type": "IfCondition",
						"dependsOn": [
							{
								"activity": "Copy data to bronze",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@greater(activity('Copy data to bronze').output.dataRead, 0)",
								"type": "Expression"
							},
							"ifFalseActivities": [
